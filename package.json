{
  "name": "remotion-template",
  "version": "0.4.1",
  "scripts": {
    "dev": "remotion preview",
    "render:mp4": "remotion render ${npm_config_video} out/${npm_config_video}.mp4",
    "render:gif": "remotion render ${npm_config_video} out/${npm_config_video}.gif",
    "test:lint": "eslint . --ext=.ts,.tsx --max-warnings=0 --format=compact",
    "test:types": "tsc",
    "test:format": "prettier --check .",
    "upgrade": "npm i $(npm outdated | cut -d' ' -f 1 | sed '1d' | xargs -I '$' echo '$@latest' | xargs echo) --save-exact"
  },
  "dependencies": {
    "@remotion/cli": "4.0.83",
    "@remotion/google-fonts": "4.0.83",
    "clsx": "2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remotion": "4.0.83"
  },
  "devDependencies": {
    "@remotion/eslint-config": "4.0.83",
    "@remotion/eslint-plugin": "4.0.83",
    "@remotion/tailwind": "4.0.83",
    "@types/react": "18.2.46",
    "@types/web": "0.0.133",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-xo": "0.43.1",
    "eslint-config-xo-react": "0.27.0",
    "eslint-config-xo-typescript": "1.0.1",
    "eslint-plugin-tailwindcss": "3.13.1",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.10",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3"
  }
}
